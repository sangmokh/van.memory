structure (RecallLastMemoryResponse) {
  description (The resulting output after recalling most recent memory)
  property (success) {
    type (SuccessConcept)
    min (Required) max (One)
  }
  property (speech) {
    type (SpeechConcept)
    min (Required) max (One)
  }
  property (memories) {
    type (Memory)
    min (Optional) max (Many)
  }
}
