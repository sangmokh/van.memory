capsule {
  id (van.memory)
  version (1.4.2)
  format (3)
  targets {
    target (bixby-mobile-en)
    target (bixby-mobile-es)
    target (bixby-mobile-fr)
  }
  permissions {
    bixby-user-id-access
  }
  capsule-imports {
    import (viv.core) {
      as (core)
    }
  }
  runtime-version (2)
  store-sections {
    section (Productivity)
    section (Utilities)
  }
  default-action (HelpAction)
  device-models {
    all
  }
  store-countries {
    all
  }
}
